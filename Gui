	local args = {
		[1] = "200PlayerGiveSpeed",
		[2] = "200"
	}

	game:GetService("ReplicatedStorage").G4444.FD.SS.ED:FindFirstChild("18Player18sCa18r"):FireServer(unpack(args))

end)
	



